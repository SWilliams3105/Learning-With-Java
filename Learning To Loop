

public class Looping {

	public static void main(String[] args) {
		// Learning to loop


		int count = 0;
		System.out.println("count\ti\tj\n");
		for (int i = 1; i<=2; i++){
			{
				for(int j = 1; j <=10; j++) {
					count = count +1;
					System.out.println(count +"\t" + i+ "\t" + j + "\n");
				}
			}

		
			System.out.println("count = " + count);

		}
	}
}
